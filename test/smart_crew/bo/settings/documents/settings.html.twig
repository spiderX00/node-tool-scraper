{% extends "SkwebIncludeBundle:Include:base_admin.html.twig" %}
                {% block Skweb_content %}
                  {%set id_c=app.request.query.get("id_c")%}
                  {%if(is_granted("ROLE_ADMIN"))%}
                      {{ render(controller("SkwebAdminpageBundle:AdminCrud:menuAdmin",{'id_c':id_c})) }}
                  {%endif%}
                    <div ui-view="mainView" class="page-content page-relative nga-default nga-stagger nga-fade" style=""><accordion-group binding-field="model.accordion-groupTitle" initial-state="open" class=""> <div class="accordion-group accordion-group-default"><div class="accordion-group-heading "><h3 class="accordion-group-title"><font><font>user verification settings</font></font></h3></div><div class="accordion-group-body ">
    <form name="formTerm" class="css-form ng-valid-ng-max ng-valid-ng-min ng-valid-required" novalidate="">
        <div class="row-fluid">
            <div class="span6">
                <field-set-accordion-group binding-field="salva nuovi termini" class=""><fieldset class="scheduler-border"><legend class="scheduler-border-label"><i class=""></i><font><font> saves the new terms</font></font></legend><div>
                    <div class="control-group has-feedback" ng-class="{&apos;has-error&apos; : fieldForm[name].$invalid &amp;&amp; fieldForm[name].$touched}" name="Versione dei termini" init="1.3" model="model.a_terms" require="true" spanlabel="spansm-12" spaninput="spansm-12"><ng-form name="fieldForm" class="ng-valid-ng-max ng-valid-ng-min ng-valid-required"><label class="spansm-12 control-label"><font><font>Version of the terms </font></font><span ng-show="true" class=""><font><font>*</font></font></span></label><div class="spansm-12"><input type="text" autocomplete="off" name="Versione dei termini" tooltip-enable="true" tooltip-trigger="focus" tooltip-placement="top" tooltip="" tooltip-class="infoTooltipClass" pattern="" class="input-block-level ng-valid-ng-max ng-valid-ng-min ng-valid-required" placeholder="Versione dei termini" required="required"></div></ng-form></div>
                    <button type="submit" class="btn btn-primary pull-right"><font><font>
                        Save
                    </font></font></button>
                </div></fieldset></field-set-accordion-group>
            </div>
        </div>
    </form>
    <form name="form" class="css-form ng-valid-min" novalidate="">
        
        <div class="row-fluid">
            <div class="span6">
                <field-set-accordion-group binding-field="Documenti d&apos;identit&#xE0;" class=""><fieldset class="scheduler-border"><legend class="scheduler-border-label"><i class=""></i><font><font> Identity documents</font></font></legend><div>
                    <div class="span6">
                        <div class="checkbox">

                                <input type="checkbox" id="identity_doc1" class="" checked="checked">
                            <label for="identity_doc1" class=""><font><font>
                                Identity card&apos;
                            </font></font></label>
                        </div>
                    </div><div class="span6">
                        <div class="checkbox">

                                <input type="checkbox" id="identity_doc2" class="" checked="checked">
                            <label for="identity_doc2" class=""><font><font>
                                driving license
                            </font></font></label>
                        </div>
                    </div><div class="span6">
                        <div class="checkbox">

                                <input type="checkbox" id="identity_doc3" class="" checked="checked">
                            <label for="identity_doc3" class=""><font><font>
                                Passport
                            </font></font></label>
                        </div>
                    </div><div class="span6">
                        <div class="checkbox">

                                <input type="checkbox" id="identity_doc4" class="" checked="checked">
                            <label for="identity_doc4" class=""><font><font>
                                Acknowledgm card. </font><font>Mod.AT
                            </font></font></label>
                        </div>
                    </div><div class="span6">
                        <div class="checkbox">

                                <input type="checkbox" id="identity_doc5" class="" checked="checked">
                            <label for="identity_doc5" class=""><font><font>
                                Acknowledgm card. </font><font>Mod.BT
                            </font></font></label>
                        </div>
                    </div>
                </div></fieldset></field-set-accordion-group>
            </div>
            <div class="span6">
                <field-set-accordion-group binding-field="Altri documenti" class=""><fieldset class="scheduler-border"><legend class="scheduler-border-label"><i class=""></i><font><font> Other documents</font></font></legend><div>
                    <div class="span6">
                        <div class="checkbox">

                                <input type="checkbox" id="other_doc" class="">
                            <label for="other_doc" class=""><font><font>
                                Gaming contract
                            </font></font></label>
                        </div>
                    </div><div class="span6">
                        <div class="checkbox">

                                <input type="checkbox" id="other_doc" class="">
                            <label for="other_doc" class=""><font><font>
                                Credit card
                            </font></font></label>
                        </div>
                    </div>
                </div></fieldset></field-set-accordion-group>
            </div>
        </div>
        <div class="row-fluid">
            <div class="span6">
                <field-set-accordion-group binding-field="Durata conti senza verifica" class=""><fieldset class="scheduler-border"><legend class="scheduler-border-label"><i class=""></i><font><font> Duration accounts without verification</font></font></legend><div>
                    <div class="span12"><font><font>
                    Keep active account </font></font><input class="input-block-level span2 ng-valid-min" type="number" min="0"><font><font> days (ND)
                    </font></font></div>
                    <div class="span12">
                        <strong class=""><font><font>On expiry of the period:</font></font></strong>
                        <div class="span12">
                            <div class="checkbox">

                                    <input type="checkbox" id="disable_login" class="">
                                <label for="disable_login" class=""><font><font>
                                    Inhibit Login
                                </font></font></label>
                            </div>
                        </div>
                        <div class="span12">
                            <div class="checkbox">

                                    <input type="checkbox" id="disable_game" class="" checked="checked">
                                <label for="disable_game" class=""><font><font>
                                Disable property of play, deposit and withdrawal
                                </font></font></label>
                            </div>
                        </div>
                    </div>
                </div></fieldset></field-set-accordion-group>
            </div>
        </div>
        <div class="row-fluid">
            <div class="span12">
                <field-set-accordion-group binding-field="Alla verifica abilita seguenti propriet&#xC3;&#xA0;" class=""><fieldset class="scheduler-border"><legend class="scheduler-border-label"><i class=""></i><font><font> Verification enables these properties&#xA0;</font></font></legend><div>
                    <div class="row-fluid">
                        <div class="span6">
                        <field-set-accordion-group binding-field="Versamenti" class=""><fieldset class="scheduler-border"><legend class="scheduler-border-label"><i class=""></i><font><font> payments</font></font></legend><div>
                            <div class="span6">
                                <div class="checkbox">

                                        <input type="checkbox" id="enable_properties8" class="" checked="checked">
                                    <label for="enable_properties8" class="">
                                    Skrill Deposit
                                    </label>
                                </div>
                            </div><div class="span6">
                                <div class="checkbox">

                                        <input type="checkbox" id="enable_properties10" class="" checked="checked">
                                    <label for="enable_properties10" class="">
                                    Aci Deposit
                                    </label>
                                </div>
                            </div><div class="span6">
                                <div class="checkbox">

                                        <input type="checkbox" id="enable_properties15" class="" checked="checked">
                                    <label for="enable_properties15" class="">
                                    Bank Deposit
                                    </label>
                                </div>
                            </div>
                        </div></fieldset></field-set-accordion-group>
                        </div>
                        <div class="span6">
                        <field-set-accordion-group binding-field="Prelievi" class=""><fieldset class="scheduler-border"><legend class="scheduler-border-label"><i class=""></i><font><font> withdrawals</font></font></legend><div>
                            <div class="span6">
                                <div class="checkbox">

                                        <input type="checkbox" id="enable_properties9" class="" checked="checked">
                                    <label for="enable_properties9" class="">
                                    Skrill Withdrawal
                                    </label>
                                </div>
                            </div><div class="span6">
                                <div class="checkbox">

                                        <input type="checkbox" id="enable_properties11" class="" checked="checked">
                                    <label for="enable_properties11" class="">
                                    Aci Withdrawal
                                    </label>
                                </div>
                            </div><div class="span6">
                                <div class="checkbox">

                                        <input type="checkbox" id="enable_properties16" class="" checked="checked">
                                    <label for="enable_properties16" class="">
                                    Bank Withdrawal
                                    </label>
                                </div>
                            </div>
                        </div></fieldset></field-set-accordion-group>
                        </div>
                    </div>
                </div></fieldset></field-set-accordion-group>
            </div>
        </div>
        <div class="row-fluid">
            <div class="span12">
                <button type="submit" class="btn btn-primary pull-right"><font><font>
                    Save
                </font></font></button>
            </div>
        </div>
    </form>
</div></div></accordion-group></div>
                {% endblock Skweb_content %}