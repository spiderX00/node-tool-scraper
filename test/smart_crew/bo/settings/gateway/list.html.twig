<div id="uiViewContainer" class="span12">
                    <!-- uiView: mainView --><div ui-view="mainView" class="page-content page-relative nga-default nga-stagger nga-fade ng-scope"><accordion-group binding-field="model.page.page_data.page_title" class="ng-scope ng-isolate-scope"> <div class="accordion-group accordion-group-default"><div class="accordion-group-heading " ng-dblclick="$event.preventDefault();" ng-click="show1()"><h3 class="accordion-group-title ng-binding"><font><font>Payment Gateway State</font></font></h3></div><div class="accordion-group-body " ng-transclude="" ng-class="{'collapse': model.collapsed === true}">
    <div class="row-fluid ng-scope">
        <div class="spansm-12">
            <div class="table-responsive">
                <table class="table table-striped table-bordered table-hover">
                    <thead>
                        <tr>
                            <th class="ng-binding">Gateway</th>
                            <th class="ng-binding"><font><font>Storage Enabled</font></font></th>
                            <th class="ng-binding"><font><font>withdrawal Enabled</font></font></th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        <!-- ngRepeat: item in model.page.page_value.gateway --><!-- end ngRepeat: item in model.page.page_value.gateway --><tr ng-repeat="item in model.page.page_value.gateway" class="ng-scope">
