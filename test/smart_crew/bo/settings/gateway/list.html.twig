                            <td>
                                <div class="checkbox">
                                    <input type="checkbox" ng-model="item.deposit_enabled" ng-true-value="1" ng-false-value="0" ng-checked="item.deposit_enabled" ng-change="changeState('dsds',item)" class="ng-pristine ng-untouched ng-valid">
                                    <label></label>
                                </div>
                            </td>
                            <td>
                                <div class="checkbox">
                                <input type="checkbox" ng-model="item.withdraw_enabled" ng-checked="item.withdraw_enabled" ng-true-value="1" ng-false-value="0" ng-change="changeState('sdsd',item)" class="ng-pristine ng-untouched ng-valid">
                                    <label></label>
                                </div>
                            </td>
                            <td>
                                <button class="btn btn-primary btn-small ng-binding" ng-click="openModal(item)"><font><font>
                                    modification
                                </font></font></button>
                            </td>
                        </tr><!-- end ngRepeat: item in model.page.page_value.gateway --><tr ng-repeat="item in model.page.page_value.gateway" class="ng-scope">
                            <td class="ng-binding"><font><font>Bank transfer</font></font></td>
                            <td>
                                <div class="checkbox">
                                    <input type="checkbox" ng-model="item.deposit_enabled" ng-true-value="1" ng-false-value="0" ng-checked="item.deposit_enabled" ng-change="changeState('dsds',item)" class="ng-pristine ng-untouched ng-valid">
                                    <label></label>
                                </div>
                            </td>
                            <td>
                                <div class="checkbox">
                                <input type="checkbox" ng-model="item.withdraw_enabled" ng-checked="item.withdraw_enabled" ng-true-value="1" ng-false-value="0" ng-change="changeState('sdsd',item)" class="ng-pristine ng-untouched ng-valid">
                                    <label></label>
                                </div>
                            </td>
                            <td>
                                <button class="btn btn-primary btn-small ng-binding" ng-click="openModal(item)"><font><font>
                                    modification
                                </font></font></button>
                            </td>
                        </tr><!-- end ngRepeat: item in model.page.page_value.gateway -->
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div></div></accordion-group>

<script type="text/ng-template" id="modalGatewaySetting.html" class="ng-scope">
    <div class="modal-header">
        <h3 class="modal-title">{{model.gateway.gateway}}</h3>
    </div>
    <form class="form-inline" ng-class="{ 'show-errors': showErrors }" ng-submit="save()" name="form" class="css-form" novalidate>
    <div class="modal-body">
            <div class="row-fluid">
                <form-errors ng-if="!form.$valid"  form="form" class="ul.unstyled"></form-errors>
                    <div class="spansm-6">
                        <field-set-accordion-group binding-field="Costi sul versamento (ND)">

                                <div class="spansm-12">
                                        <div class="radio">
                                                <input type="radio"
                                                       name="deposit_type"
                                                       ng-model="model.gateway.deposit_type"
                                                       ng-value="1">
                                            <label>
                                                {{model.page.page_data.no_cost.title}}
                                            </label>
                                        </div>
                                </div>
                                <div class="spansm-12">
                                    <div class="radio">

                                            <input type="radio"
                                                   name="deposit_type"
                                                   ng-model="model.gateway.deposit_type"
                                                   ng-value="2">
                                        <label></label>
                                    </div>
                                    <div class="control-group">
                                        <label for="cost_fixed_deposit">{{model.page.page_data.cost_fixed.title}} ({{$root.model.systemCurrency.code}})</label>
                                        <input type="text" pattern="^[0-9]\d*(\.\d+)?$" ng-min="0" ng-model="model.gateway.d_cost_fixed" style="width:80px" class="input-block-level" id="cost_fixed_deposit">
                                    </div>

                                </div>
                                <div class="spansm-12">
                                    <div class="radio">

                                            <input type="radio"
                                                   name="deposit_type"
                                                   ng-model="model.gateway.deposit_type"
                                                   ng-value="3">
                                        <label></label>
                                    </div>
                                    <div class="control-group">
                                        <label for="perc_from_deposit">{{model.page.page_data.perc_from.title}}</label>
                                        <input type="text" pattern="[0-9]+(\.[0-9][0-9]?)?" ng-min="0" ng-max="100" ng-model="model.gateway.d_perc_from" style="width:80px"
                                               class="input-block-level" id="perc_from_deposit">
                                    </div>
                                    <div class="control-group">
                                        <label for="perc_to_deposit">{{model.page.page_data.perc_to.title}}</label>
                                        <input type="text" pattern="[0-9]+(\.[0-9][0-9]?)?" ng-min="0" ng-max="100" ng-model="model.gateway.d_perc_to" style="width:80px"
                                               class="input-block-level" id="perc_to_deposit">
                                    </div>

                                </div>
                                <div class="spansm-12">
                                    <label for="deposit_min">{{model.page.page_data.deposit_min.title}} ({{$root.model.systemCurrency.code}})</label>
                                    <input type="text"  ng-required="true" pattern="^[0-9]\d*(\.\d+)?$" ng-model="model.gateway.deposit_min" style="width:80px"
                                           class="input-block-level" id="deposit_min">
                                </div>

                        </field-set-accordion-group>
                    </div>
                    <div class="spansm-6">
                        <field-set-accordion-group binding-field="Costi sul prelievo (ND)">

                                <div class="spansm-12">
                                    <div class="radio">

                                            <input type="radio"
                                                   name="withdraw_type"
                                                   ng-model="model.gateway.withdraw_type"
                                                   ng-value="1">
                                        <label>
                                            {{model.page.page_data.no_cost.title}}
                                        </label>
                                    </div>
                                </div>
                                <div class="spansm-12">
                                    <div class="radio">

                                            <input type="radio"
                                                   name="withdraw_type"
                                                   ng-model="model.gateway.withdraw_type"
                                                   ng-value="2">
                                        <label></label>
                                    </div>
                                    <div class="control-group">
                                        <label for="cost_fixed_withdraw">{{model.page.page_data.cost_fixed.title}} ({{$root.model.systemCurrency.code}})</label>
                                        <input type="text" pattern="^[0-9]\d*(\.\d+)?$" ng-model="model.gateway.w_cost_fixed" style="width:80px"
                                               class="input-block-level" id="cost_fixed_withdraw">
                                    </div>

                                </div>
                                <div class="spansm-12">
                                    <div class="radio">

                                            <input type="radio"
                                                    name="withdraw_type"
                                                    ng-model="model.gateway.withdraw_type"
                                                    ng-value="3">
                                        <label></label>
                                    </div>
                                    <div class="control-group">
                                        <label for="perc_from_withdraw">{{model.page.page_data.perc_from.title}}</label>
                                        <input type="text" pattern="[0-9]+(\.[0-9][0-9]?)?" ng-max="100" ng-model="model.gateway.w_perc_from" style="width:80px"
                                               class="input-block-level" id="perc_from_withdraw">
                                    </div>
                                    <div class="control-group">
                                        <label for="perc_to_withdraw">{{model.page.page_data.perc_to.title}}</label>
                                        <input type="text" pattern="[0-9]+(\.[0-9][0-9]?)?" ng-max="100" ng-model="model.gateway.w_perc_to" style="width:80px"
                                               class="input-block-level" id="perc_to_withdraw">
                                    </div>
                                </div>
                                <div class="spansm-12">
                                    <label for="withdraw_min">{{model.page.page_data.withdraw_min.title}} ({{$root.model.systemCurrency.code}})</label>
                                    <input type="text" ng-required="true" pattern="^[0-9]\d*(\.\d+)?$"  ng-model="model.gateway.withdraw_min" style="width:80px"
                                           class="input-block-level" id="withdraw_min">
                                </div>

                        </field-set-accordion-group>
                    </div>


            </div>


        <!--/form-->

    </div>

    <div class="modal-footer">
        <button type="submit"
                class="btn btn-primary"
                ng-disabled="form.$invalid">{{model.page.page_data.modify.title}}</button>
        <button type="button"
                class="btn btn-info"
                ng-click="closeModal()">Cancella (ND)</button>
    </div>
    </form>
</script>
</div>
                </div>